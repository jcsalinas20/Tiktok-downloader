<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.TextFlow?>

<AnchorPane maxHeight="600.0" maxWidth="864.0" minHeight="243.0" minWidth="712.0" prefHeight="448.0" prefWidth="714.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.downloadList.ImportFile">
  <Label layoutX="37.0" layoutY="26.0" text="File to Import:">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Label>
  <TextField fx:id="tfPath" editable="false" layoutX="33.0" layoutY="56.0" prefHeight="32.0" prefWidth="517.0" promptText="absolute path">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </TextField>
  <Button fx:id="btnExamine" alignment="CENTER" layoutX="560.0" layoutY="56.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="125.0" text="Examine">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Button>
    <Button fx:id="btnImport" alignment="CENTER" layoutX="165.0" layoutY="402.0" mnemonicParsing="false" onAction="#btnImportOnClick" prefHeight="32.0" prefWidth="125.0" text="Import">
        <font>
            <Font name="Comic Sans MS" size="15.0" />
        </font>
    </Button>
  <Button fx:id="btnCancel" alignment="CENTER" layoutX="423.0" layoutY="402.0" mnemonicParsing="false" onAction="#btnCancelOnClick" prefHeight="32.0" prefWidth="125.0" text="Close">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Button>
  <ScrollPane fx:id="scrollPane" layoutX="30.0" layoutY="126.0" prefHeight="265.0" prefWidth="662.0" styleClass="text-flow-panel" stylesheets="@../../css/application.css">
     <TextFlow fx:id="tFlowImports" prefHeight="263.0" prefWidth="647.0" styleClass="text-flow" stylesheets="@../../css/application.css">
        <padding>
           <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
        </padding>
     </TextFlow>
  </ScrollPane>
  <CheckBox fx:id="cbIgnore" layoutX="152.0" layoutY="95.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="411.0" selected="true" text="Ignore broken Urls and other random text in the file">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </CheckBox>
</AnchorPane>
