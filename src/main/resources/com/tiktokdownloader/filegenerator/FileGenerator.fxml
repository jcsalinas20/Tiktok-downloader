<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="600.0" maxWidth="864.0" minHeight="578.0" minWidth="720.0" prefHeight="598.0" prefWidth="851.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tiktokdownloader.filegenerator.FileGenerator">
    <MenuBar layoutY="2.0" stylesheets="@../application.css" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Menu fx:id="menuDownloadList" mnemonicParsing="false" />
        <Menu fx:id="menuAllUserVideos" mnemonicParsing="false" />
        <Menu fx:id="menuOneVideo" mnemonicParsing="false" />
        <Menu fx:id="menuFileGenerator" mnemonicParsing="false" />
        <Menu fx:id="menuLogs" mnemonicParsing="false" />
        <styleClass>
            <String fx:value="menu-text" />
        </styleClass>
    </MenuBar>
   <Label layoutX="328.0" layoutY="50.0" text="Text File Generator">
      <font>
         <Font name="Comic Sans MS Bold Italic" size="20.0" />
      </font>
   </Label>
    <Label layoutX="42.0" layoutY="112.0" prefHeight="22.0" prefWidth="132.0" text="Tiktok username:">
        <font>
            <Font name="Comic Sans MS" size="15.0" />
        </font>
    </Label>
    <TextField fx:id="tfTikTokName" layoutX="182.0" layoutY="107.0" prefHeight="32.0" prefWidth="268.0" promptText="without @">
        <font>
            <Font name="Comic Sans MS" size="15.0" />
        </font>
    </TextField>
    <ImageView fx:id="imageLogo" fitHeight="72.0" fitWidth="364.0" layoutX="482.0" layoutY="76.0" pickOnBounds="true" preserveRatio="true" />
    <Label layoutX="42.0" layoutY="172.0" text="Enter destination folder:">
        <font>
            <Font name="Comic Sans MS" size="15.0" />
        </font>
    </Label>
    <TextField fx:id="tfPath" layoutX="235.0" layoutY="167.0" prefHeight="32.0" prefWidth="429.0" promptText="absolute path">
        <font>
            <Font name="Comic Sans MS" size="15.0" />
        </font>
    </TextField>
    <Button fx:id="btnExamine" alignment="CENTER" layoutX="675.0" layoutY="167.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="125.0" text="Examine">
        <font>
            <Font name="Comic Sans MS" size="15.0" />
        </font>
    </Button>
   <Button fx:id="btnCreateFile" alignment="CENTER" layoutX="42.0" layoutY="219.0" mnemonicParsing="false" onAction="#onClickGenerateFile" prefHeight="32.0" prefWidth="125.0" text="Create File">
      <font>
         <Font name="Comic Sans MS" size="15.0" />
      </font>
   </Button>
   <Button fx:id="btnRestart" alignment="CENTER" disable="true" layoutX="675.0" layoutY="219.0" mnemonicParsing="false" onAction="#onClickRestart" prefHeight="32.0" prefWidth="125.0" text="Restart">
       <font>
           <Font name="Comic Sans MS" size="15.0" />
       </font>
   </Button>
  <ScrollPane fx:id="scrollPane" layoutX="42.0" layoutY="265.0" prefHeight="278.0" prefWidth="758.0" styleClass="text-flow-panel" stylesheets="@../application.css">
     <TextFlow fx:id="tFlowImports" prefHeight="263.0" prefWidth="647.0" styleClass="text-flow" stylesheets="@../application.css">
        <padding>
           <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
        </padding>
     </TextFlow>
  </ScrollPane>
   <ComboBox fx:id="cbTypeFile" layoutX="351.0" layoutY="219.0" prefHeight="32.0" prefWidth="150.0" promptText="File Generator" />
</AnchorPane>
