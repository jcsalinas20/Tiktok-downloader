<AnchorPane maxHeight="600.0" maxWidth="864.0" minHeight="578.0" minWidth="720.0" prefHeight="598.0" prefWidth="851.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.downloadList.DownloaderList">
    <MenuBar layoutY="2.0" stylesheets="@../../css/application.css" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Menu fx:id="menuDownloadList" mnemonicParsing="false" />
        <Menu fx:id="menuAllUserVideos" mnemonicParsing="false" />
        <Menu fx:id="menuOneVideo" mnemonicParsing="false" />
      <styleClass>
         <String fx:value="menu-text" />
      </styleClass>
    </MenuBar>
  <Label layoutX="48.0" layoutY="64.0" prefHeight="22.0" prefWidth="158.0" text="Name of user TikTok:">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Label>
  <TextField fx:id="tfTikTokName" layoutX="213.0" layoutY="59.0" onKeyReleased="#tfNameKeyReleased" prefHeight="32.0" prefWidth="268.0" promptText="without @">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </TextField>
  <ImageView fx:id="imageLogo" fitHeight="72.0" fitWidth="364.0" layoutX="496.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true" />
  <Label layoutX="31.0" layoutY="125.0" text="Enter destination folder:">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Label>
  <TextField fx:id="tfPath" layoutX="222.0" layoutY="120.0" onKeyReleased="#tfPathKeyReleased" prefHeight="32.0" prefWidth="456.0" promptText="absolute path">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </TextField>
    <Accordion layoutX="690.0" layoutY="217.0" />
    <ListView fx:id="listLinks" layoutX="26.0" layoutY="165.0" prefHeight="289.0" prefWidth="799.0" styleClass="normal-text" stylesheets="@../../css/application.css" />
  <Label fx:id="lblError" alignment="CENTER" layoutX="157.0" layoutY="462.0" prefHeight="22.0" prefWidth="534.0" textAlignment="CENTER" textFill="#f82c2c">
     <font>
        <Font name="Comic Sans MS" size="13.0" />
     </font>
  </Label>
  <Button fx:id="btnDeleteRow" layoutX="26.0" layoutY="491.0" mnemonicParsing="false" onAction="#btnDeleteRowOnClick" prefHeight="32.0" prefWidth="118.0" text="Delete Row">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Button>
  <TextField fx:id="tfUrl" alignment="CENTER" layoutX="159.0" layoutY="491.0" prefHeight="32.0" prefWidth="534.0" promptText="url">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </TextField>
    <Button fx:id="btnInsertRow" layoutX="708.0" layoutY="491.0" mnemonicParsing="false" onAction="#btnInsertRowOnClick" prefHeight="32.0" prefWidth="118.0" text="Insert Row">
        <font>
            <Font name="Comic Sans MS" size="15.0" />
        </font>
    </Button>
    <Button fx:id="btnStartScraping" alignment="CENTER" layoutX="119.0" layoutY="545.0" mnemonicParsing="false" onAction="#btnDownloadListOnClick" text="Download List">
        <font>
            <Font name="Comic Sans MS" size="15.0" />
        </font>
    </Button>
  <Button fx:id="btnImportFile" alignment="CENTER" layoutX="363.0" layoutY="545.0" mnemonicParsing="false" onAction="#btnImportFileOnClick" prefHeight="32.0" prefWidth="125.0" text="Import File">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Button>
  <Button fx:id="btnSaveAsFile" alignment="CENTER" layoutX="601.0" layoutY="545.0" mnemonicParsing="false" onAction="#btnSaveAsFileOnClick" prefHeight="32.0" prefWidth="125.0" text="Save As File">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Button>
  <Button fx:id="btnExamine" alignment="CENTER" layoutX="700.0" layoutY="120.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="125.0" text="Examine">
     <font>
        <Font name="Comic Sans MS" size="15.0" />
     </font>
  </Button>
</AnchorPane>
